<%- include("partials/header") %>
<body>

<form action="/login/todo" method="post">
<section class="vh-100" style="background-color: #508bfc;">
    <div class="container py-5 h-100">
      <div class="row d-flex justify-content-center align-items-center h-100">
        <div class="col-12 col-md-8 col-lg-6 col-xl-5">
          <div class="card shadow-2-strong" style="border-radius: 1rem;">
            <div class="card-body p-5 text-center">
  
              <h3 class="mb-5">Log in Admin</h3>
  
              <div class="form-outline mb-4">
                <input type="name" id="typeEmailX-2" name="username" class="form-control form-control-lg" required />
                <label class="form-label" for="typeEmailX-2"> Username</label>
              </div>
  
              <div class="form-outline mb-4">
                <input type="password" id="typePasswordX-2" name="password" class="form-control form-control-lg" required/>
                <label class="form-label" for="typePasswordX-2">Password</label>
              </div>
  
              
              <button type="submit" name="btn" class="btn btn-primary btn-lg btn-block" type="submit">Login</button>
              <a href="register">Register</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</form>
<%- include ("./partials/footer")%>